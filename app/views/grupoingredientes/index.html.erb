<p id="notice"><%= notice %></p>

<h1>Grupoingredientes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grupoingredientes.each do |grupoingrediente| %>
      <tr>
        <td><%= grupoingrediente.nombre %></td>
        <td><%= link_to 'Show', grupoingrediente %></td>
        <td><%= link_to 'Edit', edit_grupoingrediente_path(grupoingrediente) %></td>
        <td><%= link_to 'Destroy', grupoingrediente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

